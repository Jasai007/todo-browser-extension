{
    "manifest_version": 3,
    "name": "To-Do List Extension",
    "version": "1.0",
    "description": "A simple To-Do list Chrome extension.",
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/images.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    }
}
